package ejercicio1;

import java.io.IOException;
import java.util.ArrayList;
import java.util.List;

public class EjemploProcessBuilder {

	public static void main(String[] args) throws IOException {
		List<String> programas = new ArrayList<String>();
		ProcessBuilder notas = new ProcessBuilder("notepad");
		Process p1 = notas.start();

		ProcessBuilder calculadora = new ProcessBuilder("calc");
		Process p2 = calculadora.start();

		System.out.println(p1.info());
		System.out.println(p1.isAlive());

	}
}
