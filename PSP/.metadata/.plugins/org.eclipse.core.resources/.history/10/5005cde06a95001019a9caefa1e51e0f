package ejercicio2;

import java.io.IOException;

public class EjercicioProcessBuilderPing {
	public static void main("ping 192.168.10.52") throws IOException, InterruptedException {
		args[0] = ;
		ProcessBuilder pb = new ProcessBuilder(args[0]);
		Process p1 = pb.start();
		p1.wait(10000);
		if (p1.exitValue() == 0) {
			System.out.println("El ping ha sido exitoso");
		} else {
			System.out.println("El ping NO ha sido exitoso");
		}

	}
}
